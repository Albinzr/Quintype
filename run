#! /bin/sh

git add .
echo "Enter Commit Message: $name"
read commitMessage
git commit -m "$commitMessage"
currentTag=$(git describe --abbrev=0 --tags)
incrementer=$(awk(0.01))
newTag=$incrementer+$(git describe --abbrev=0 --tags) | bc -l
echo $incrementer+$(git describe --abbrev=0 --tags) | bc -l


